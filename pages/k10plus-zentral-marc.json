          "required": true,
          "pattern": "^(GBV_USEFLAG_.+|SYSFLAG_.+|.*|GBV_ILN_.+|FID|SSG-OLC-.+|SSG-OPC-.+|ISI-DE-.+|GBV_KXP|GBV_OLC)$"
